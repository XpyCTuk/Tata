extends ../layout/layout.pug

block content   

    //.popup-container
      .popup
        .close-popup 
          include ../../images/img/popup-close.svg      
        .popup-header Прайс-лист на архитектурные работы 
        .popup-body Проектные работы, m2 
        .popup-body-item ЭП (эскизное предложение) <span class="prise-style_1">200 ₽</span>
        .popup-body-item АР (архитектурные решения) <span class="prise-style_2">200 ₽</span>
        .popup-body-item-last КР (конструктивные решения) <span class="prise-style_3">200 ₽</span>
