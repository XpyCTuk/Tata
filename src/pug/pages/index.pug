        img.site-picture-2(src='../../images/img/2.png', alt="")
        img.site-picture-3(src='../../images/img/3.png', alt="")
        img.site-picture-4(src='../../images/img/4.png', alt="")
        img.site-picture-5(src='../../images/img/5.png', alt="")
        img.site-picture-6(src='../../images/img/6.png', alt="")
